<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.imsw.model.Author" />
	<import class="com.imsw.model.Book" />
	<import class="com.imsw.model.Borrower" />
	<import class="com.imsw.model.Loan" />
	<class name="com.imsw.model.impl.AuthorImpl" table="BB_Author">
		<cache usage="read-write" />
		<id name="authorId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.imsw.model.impl.BookImpl" table="BB_Book">
		<cache usage="read-write" />
		<id name="bookId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="cost" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="authorId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.imsw.model.impl.BorrowerImpl" table="BB_Borrower">
		<cache usage="read-write" />
		<id name="borrowerId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="city" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="PhoneNo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.imsw.model.impl.LoanImpl" table="BB_Loan">
		<cache usage="read-write" />
		<id name="loanId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="dateStart" type="org.hibernate.type.TimestampType" />
		<property name="dateFinish" type="org.hibernate.type.TimestampType" />
		<property name="borrowerId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="bookId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>